{
    "name": "hyperf/http-message",
    "description": "microservice framework base on swoole",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "http-message"
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/codec": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/coroutine": "~3.1.0",
        "hyperf/engine": "^2.11",
        "hyperf/support": "~3.1.0",
        "psr/http-message": "^1.0 || ^2.0",
        "swow/psr7-plus": "^1.0"
    },
    "suggest": {
        "psr/container": "Required to replace RequestParserInterface."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\HttpMessage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\HttpMessage\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\HttpMessage\\ConfigProvider"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^12.3",
        "psr/container": "^2.0",
        "swoole/ide-helper": "^6.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
